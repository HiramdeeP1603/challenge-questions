        "L = [\"Ram\",1,\"Shayam\",2,\"Aman\",3]\n",
        "print(L)\n",
        "L = [str(i) for i in L]\n",
        "L.sort()\n",
        "\n",
        "L1 = []\n",
        "for i in L:\n",
        "  try:\n",
        "    L1.append(int(i))\n",
        "  except:\n",
        "    L1.append(i)\n",
